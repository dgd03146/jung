{
  "name": "@jung/ai-translator",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./prompts": {
      "types": "./dist/prompts.d.ts",
      "default": "./dist/prompts.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "check": "tsc --noEmit",
    "lint": "pnpm biome lint . --apply",
    "format": "pnpm biome format --write ."
  },
  "devDependencies": {
    "@jung/configs": "workspace:*",
    "@jung/typescript-config": "workspace:*",
    "@types/node": "^22.10.5",
    "dotenv": "^16.4.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@google/genai": "^1.38.0"
  }
}
